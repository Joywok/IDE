{
  "private": true,
  "version": "0.0.1",
  "name": "Joywok-IDE",
  "main": "build/template/index.html",
  "serverurl": "http://jwtest-1802568048.cn-north-1.elb.amazonaws.com.cn",
  "window": {
    "icon":"build/images/ico-s.png",
    "title": "Joywok IDE",
    "toolbar": false,
    "frame": true,
    "width": 840,
    "height": 640,
    "min_height": 640
  },
  "webkit": {
    "plugin": true,
    "page-cache": false
  },
  "nodejs": true,
  "permissions": "system",
  "chromium-args": "-ignore-certificate-errors",
  "node-remote": [
    "http://127.0.0.1:10000",
    "http://192.168.1.73"
  ],
  "inject-js-start": "src/scripts/test.js",
  "webview": {
    "partitions": [
      {
        "name": "trusted",
        "accessible_resources": [
          "<all_urls>"
        ]
      },
      {
        "name": "devtools",
        "accessible_resources": [
          "<all_urls>"
        ]
      }
    ]
  },
  "no-edit-menu": false,
  "entry": {},
  "dependencies": {
    "chokidar": "^1.6.1",
    "decompress-zip": "^0.3.0",
    "fs-extra": "^1.0.0",
    "request": "^2.79.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "material-ui": "^0.16.4",
    "del": "^2.2.2",
    "path": "^0.12.7",
    "superagent": "^3.1.0"
  },
  "scripts": {
    "start": "dora --plugins \"proxy?watchDirs=./mock,webpack,webpack-hmr\"",
    "build": "atool-build",
    "test": "atool-test-mocha ./src/**/*-test.js",
    "public": "cp -R a.tgz payload package.json nwjc node_modules minidump_stackwalk chromedriver build bower_components ../public/.",
    "edit": "cp -R tmp package_1.json project.json config.json node_modules_1 build bower_components aaaaa/."
  }
}
